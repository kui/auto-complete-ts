auto-complete-ts.el
Auto Completion source for typescript for GNU Emacs


;; sample setting
(add-to-list 'load-path "~/ts/emacs")
(load "TypeScript")
(add-to-list 'auto-mode-alist '("\\.ts" . typescript-mode))
(defun my-ac-ts-mode-setup ()
  (setq ac-sources '(ac-source-ts)))
(add-hook 'typescript-mode-hook 'my-ac-ts-mode-setup)
(setq ac-ts-lib-dir "~/ts/typescript/typings/")
